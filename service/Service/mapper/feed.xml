<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="feed">  
  <select id = "selectAll">
    select * from Feed;
  </select>

  <select id = "selectOne">
    select * from Feed where f_No = #{f_No};
  </select>
</mapper>