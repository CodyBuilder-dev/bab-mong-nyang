<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="feed">  
  <select id = "selectAll">
    select * from feed;
  </select>

  <select id = "selectOne">
    select * from feed where f_No = #{f_No};
  </select>

  <insert id = "addFeed">
    insert into feed values(0, #{f_Name}, #{f_Manufacturer}, #{f_Calory}, #{f_Species});
  </insert>
 
  <update id = "updateFeed">
    update feed set f_Calory = #{f_Calory}, f_Species = #{f_Species} where f_No = #{f_No};
  </update>

  <delete id = "deleteFeed">
    delete from feed where f_No = #{f_No};
  </delete>
</mapper>