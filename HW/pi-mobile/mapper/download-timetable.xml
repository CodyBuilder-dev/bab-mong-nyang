<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample_timetable">  
  <delete id="truncAll">
    truncate table sample_timetable;
  </delete>
  <insert id="insertValue">
    INSERT INTO sample_timetable (_time, _Amount) VALUES (time,amount);
  </insert>
</mapper>